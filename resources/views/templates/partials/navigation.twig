     <div class="hamburger__patty"></div>
     <div class="hamburger__patty"></div>
     <div class="hamburger__patty"></div>
   </div>
  </button>
  <div id="navbar"class="collapse navbar-collapse">
    <ul class="nav navbar-nav ">
      {% if auth.check %}
        <li class="nav-item active">
          <a class="nav-link" href="{{path_for('home') }}"> <span>ПОЧЕТНА</span></a>
        </li>
        {% if auth.user.permissions.hasPermission('is_admin')%}
        <li class="nav-item active">
          <a class="nav-link" href="{{path_for('admin') }}"> <span>АДМИН</span></a>
        </li>
        {% endif %}
        <li class="nav-item active ">
          <a class="nav-link " href="{{path_for('update') }}"> <span>ПОСТВКИ</span></a>
