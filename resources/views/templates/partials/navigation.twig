  </div>
</nav>
{% if auth.check %}
<div class="chat-swipe"></div>
<div class="chat" data-simplebar>
  <div class="expand btn-group" role="group">
    <button type="button" class="btn btn-secondary add">ДОДАДИ</button>
    <button type="button" class="btn btn-secondary block">БЛОКИРАЈ</button>
  </div>
  <div class="content">
    {% if auth.user.contact.friends_pending|length >= 2 %}
    <label for="pending">Пријатели во исчекување...</label>
    <ul name="pending" class="pending">
      {% for friend in auth.user.contact.getFriendsPending() %}
      <li><img src="{{auth.user.get_gravatar(friend.email,40)}}"><span>{{friend.username}}</span><span style="{{auth.user.displayStatus(friend.energy.status)}}"></span> </li>
      {% endfor %}
    </ul>
    {% endif %}
    {% if auth.user.contact.friends >= 1 %}
    <label for="friends">ПРИЈАТЕЛИ</label>
    <ul name="friends" class="friends">
      {% for friend in auth.user.contact.getFriends() %}
      <li><img src="{{auth.user.get_gravatar(friend.email,40)}}"><span>{{friend.username}}</span><span style="{{auth.user.displayStatus(friend.energy.status)}}"></span> </li>
      {% endfor %}
    </ul>
    {% endif %}

    <label for="clans">ФАМИЛИИ</label>
    <ul name="clans" class="clans">
      {% for clan in auth.user.getClans() %}
      <li><img src="{{auth.user.get_gravatar(auth.user.email,40)}}"> <span>{{clan.name}}</span> <span style="background:var(--gray-dark)"></span> </li>
      {% endfor %}
    </ul>
  </div>
  <div class="search">
    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24">
      <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"/>
      <path d="M0 0h24v24H0z" fill="none"/>
    </svg>
    <input type="text" id="search" placeholder="Пребарувај...">
  </div>
</div>
<div class="sidebar-swipe"></div>
<nav class="sidebar">
  <div class="profile">
    <div class="img">
      <div class="progress blue">
        <span class="progress-left">
          <span class="progress-bar"></span>
        </span>
        <span class="progress-right">
          <span class="progress-bar"></span>
        </span>
        <div class="progress-value">
         <span class="num">...</span>
         <img src="{{auth.user.get_gravatar(auth.user.email)}}" width="92" height="92">
        </div>
      </div>
      <script type="text/javascript">
        $(function(){
          var input = {{auth.user.prom.procent()}};
          if(input == 100){input--;}
          var output = (360 * input) / 100;
          if(output <= 180){
            output1 = output;
            output2 = 0;
          }else{
            output1 = 180 ;
            output2= output - 180;
          }
          $('.progress').children('.progress-value').children('.num').html(input+'%');
          $('.progress').children('.progress-right').children('.progress-bar').css('-webkit-transform',"rotate("+output1+"deg)");
          $('.progress').children('.progress-left').children('.progress-bar').css('-webkit-transform',"rotate("+output2+"deg)");
        });
      </script>
      <div class="name">
        <span>{{auth.user.username}}</span><br>
        <span>{{auth.user.contact.clan}}</span>
      </div>
    </div>
